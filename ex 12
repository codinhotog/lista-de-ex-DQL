CREATE TABLE alunos ( 
    id NUMBER GENERATED BY DEFAULT AS IDENTITY PRIMARY KEY, 
    nome VARCHAR2(100) NOT NULL, 
    data_nascimento DATE NOT NULL 
);

INSERT INTO alunos (nome, data_nascimento) VALUES ('Jo√£o Silva', DATE '1990-04-15');

